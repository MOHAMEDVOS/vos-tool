# Environment variables for VOS Tool cloud deployment
# Copy this file to .env and fill in your values

# Database Configuration
POSTGRES_PASSWORD=your_secure_database_password_here
REDIS_PASSWORD=your_secure_redis_password_here

# ReadyMode Credentials
READYMODE_USER=your_readymode_username
READYMODE_PASSWORD=your_readymode_password

# Application Configuration
DEPLOYMENT_MODE=enterprise
FORCE_READYMODE=true
MAX_CONCURRENT_DOWNLOADS=10

# Security
SECRET_KEY=your_secret_key_for_session_management

# Optional: Custom domain and SSL
# DOMAIN_NAME=vos.yourdomain.com
# SSL_CERT_PATH=/path/to/ssl/cert.pem
# SSL_KEY_PATH=/path/to/ssl/key.pem

# AWS Configuration (for local development)
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_DEFAULT_REGION=us-east-1

# Monitoring and Logging
LOG_LEVEL=INFO
ENABLE_DEBUG=false
